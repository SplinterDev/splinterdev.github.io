{
  "name": "tkl",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc && terser ./dist/index.js -o ./dist/index.min.js",
    "serve": "http-server -S -C cert.pem -K key.pem -p 8000",
    "watch": "nodemon --ext ts,js --watch './src/*.ts' --exec 'yarn build'",
    "dev": "concurrently \"yarn watch\" \"yarn serve\""
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "terser": "^5.21.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1"
  }
}
